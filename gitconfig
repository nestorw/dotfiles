[user]
    name = Nestor Walker
    email = nestor@developdc.com

[core]
    editor = vim
    excludesfile = /Users/nestor/.gitignore_global

[color]
    branch = auto
    diff = auto
    status = auto

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green
    untracked = cyan

[alias]
    full-stash = stash --include-untracked
    ll = log --oneline -n 5
	co = checkout
	br = branch
	ci = commit
	st = status
